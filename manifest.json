{
  "manifest_version": 2,

  "name": "Nutrition Tracker",
  "description": "This extension allows the user to track calories consumed daily.",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "content_security_policy":"script-src 'self'; object-src 'self'",
  //    "browser_action": {
//      "default_icon": "icon.png",
//      "default_popup": "popup.html"
//    },
  "permissions": [
    "activeTab",
    "storage",
    "management"
  ]
}
